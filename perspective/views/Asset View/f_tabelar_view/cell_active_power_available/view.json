{
  "custom": {},
  "params": {
    "max": 30000,
    "unit": "KW",
    "value": 5.1
  },
  "propConfig": {
    "params.max": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.unit": {
      "paramDirection": "output",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 28,
      "width": 78
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Progress"
        },
        "position": {
          "height": 0.6429,
          "width": 1,
          "y": 0.1429
        },
        "propConfig": {
          "props.max": {
            "binding": {
              "config": {
                "expression": "if(isNull({view.params.max}) || {view.params.max} \u003c\u003d 0, 1, {view.params.max})\r\n"
              },
              "type": "expr"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "bar": {
            "color": "#926178C2"
          }
        },
        "type": "ia.display.progress"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "height": 0.3571,
          "rotate": {
            "anchor": "-114% 75%"
          },
          "width": 0.7949,
          "x": 0.1026,
          "y": 0.2857
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "numberFormat(toDouble({view.params.value}), \"#,##0.0\")\r\n+ if(isNull({view.params.unit}) || {view.params.unit} \u003d \"\", \"\", \" \" + {view.params.unit})\r\n\r\n\r\n\r\n\r\n\r\n"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "fontSize": 11
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "mode": "percent",
      "style": {
        "paddingLeft": 15,
        "paddingRight": 1
      }
    },
    "type": "ia.container.coord"
  }
}