{
  "custom": {},
  "params": {
    "enabled": true,
    "rowIndex": "",
    "value": 1
  },
  "propConfig": {
    "params.enabled": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.rowIndex": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 36,
      "width": 36
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\t# Send the new state back to the Table\n\tsystem.perspective.sendMessage(\n\t    \"tableSetSelection\",\n\t    {\"rowIndex\": self.view.params.rowIndex, \"value\": bool(self.props.selected)},\n\t    scope\u003d\"page\"\n\t)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Checkbox"
        },
        "position": {
          "height": 25,
          "width": 25,
          "x": 5.5,
          "y": 5.5
        },
        "propConfig": {
          "meta.visible": {
            "binding": {
              "config": {
                "expression": "!isNull({view.params.value})"
              },
              "type": "expr"
            }
          },
          "props.enabled": {
            "binding": {
              "config": {
                "path": "view.params.enabled"
              },
              "type": "property"
            }
          },
          "props.selected": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "transforms": [
                {
                  "fallback": 0,
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": 1,
                      "output": true
                    },
                    {
                      "input": 0,
                      "output": null
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "text": ""
        },
        "type": "ia.input.checkbox"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.coord"
  }
}